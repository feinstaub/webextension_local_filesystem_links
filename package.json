{
  "title": "Local Filesystem Links",
  "name": "alien-local-filesystem-links",
  "version": "0.99.61",
  "description": "Scans pages for file:/// links and makes it possible to open them with the system's file browser.",
  "main": "index.js",
  "author": "austrALIENsun",
  "scripts": {
    "start": "web-ext run -s dist/",
    "prebundle": "npm run lf-endings",
    "bundle": "web-ext build -s dist/ --overwrite-dest",
    "sign": "web-ext sign",
    "clean": "rimraf dist",
    "dev": "npm run clean && cross-env NODE_ENV=development webpack-dev-server --inline --hot --content-base=dist/",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "eslint ./src/**.js",
    "lf-endings": "crlf --set=LF dist/*.*"
  },
  "engines": {
    "firefox": ">=38.0a1",
    "fennec": ">=38.0a1"
  },
  "license": "MPL 1.1/GPL 3.0",
  "id": "jid1-JAzC7z53jemo5Q@jetpack",
  "permissions": {
    "multiprocess": true
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "match-pattern": "0.0.2",
    "vue": "^2.1.0",
    "webextension-polyfill": "^0.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "crlf": "^1.1.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.1.0",
    "eslint-config-defaults": "^9.0.0",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.7.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.21.0",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "web-ext": "^2.2.2",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "write-file-webpack-plugin": "^3.4.2"
  }
}
