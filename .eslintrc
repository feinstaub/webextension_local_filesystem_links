{
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": [],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "no-invalid-this": 0,
        "new-cap": [0, {"newIsCap": false}]
    },
    "globals": {
        "jQuery": true,
        "browser": true
    }
}
