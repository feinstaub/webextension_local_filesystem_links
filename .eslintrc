{
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "module",
    },
    "extends": [],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "no-invalid-this": 0,
        "new-cap": [0, {"newIsCap": false}],
        "indent": ["error", 4],
    },
    "globals": {
        "jQuery": true,
        "browser": true
    }
}
